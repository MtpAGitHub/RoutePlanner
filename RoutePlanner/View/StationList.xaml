<Window x:Name="wdwStationList" x:Class="StationList"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    DataContext="{Binding RelativeSource={RelativeSource Self}}"
    Title="StationList" Height="300" Width="300">
    <Grid>
        <ListBox x:Name="txtbxStationList" ItemsSource="{Binding Path=dispStationList}" HorizontalAlignment="Left" Height="205" Margin="25,44,0,0" VerticalAlignment="Top" Width="238"/>
        <Label x:Name="lblStationList" Content="List Of Stations" HorizontalAlignment="Left" Margin="17,13,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
